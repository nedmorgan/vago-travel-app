{
  "name": "vago-travel-app",
  "version": "1.0.0",
  "description": "Through this application you are able to explore different countries around the world before you book your ticket and go wandering. You can save countries that you want to visit in MongoDB.",
  "main": "index.js",
  "engines": {
    "node": "11.11.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"cd client && npm start\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "cd client && npm i && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nedmorgan/vago-travel-app.git"
  },
  "keywords": [
    "travel",
    "explore",
    "wander",
    "sightseeing",
    "trip",
    "vacation"
  ],
  "author": "Nathaniel Morgan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nedmorgan/vago-travel-app/issues"
  },
  "homepage": "https://github.com/nedmorgan/vago-travel-app#readme",
  "dependencies": {
    "concurrently": "^4.1.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "fs": "0.0.1-security",
    "mongoose": "^5.4.19",
    "morgan": "^1.9.1"
  }
}
